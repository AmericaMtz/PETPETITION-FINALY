        <servlet-name>RegistroPersonaServlet</servlet-name>
        <url-pattern>/RegistroPersona</url-pattern>
    </servlet-mapping>
    <servlet>
        <description/>
        <display-name>MAdoptaServlet</display-name>
        <servlet-name>MAdoptaServlet</servlet-name>
